{
  "name": "socalsolver-refactor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:strict": "eslint --max-warnings=0 .",
    "test:playwright": "playwright test",
    "ci": "npm run build && npm run lint:strict && npm run test:playwright",
    "lhci:collect": "lhci collect --config=./lighthouserc.json",
    "lhci:assert": "lhci assert --config=./lighthouserc.json",
    "lhci": "npm run lhci:collect && npm run lhci:assert",
    "postbuild": "node ./scripts/postbuild.js"
  },
  "dependencies": {
    "next": "16.0.0",
    "next-sitemap": "^4.2.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "^16.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "lighthouse": "^13.0.1",
    "playwright": "^1.56.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.46.2"
  }
